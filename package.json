{
  "name": "ds-ui-library",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwiyantop/ds-ui-library.git"
  },
  "homepage": "https://github.com/dwiyantop/ds-ui-library",
  "type": "module",
  "main": "dist/ds-ui-library-umd.js",
  "module": "dist/ds-ui-library-es.js",
  "types": "dist/main.d.ts",
